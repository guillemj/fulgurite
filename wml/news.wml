# $Id$

<define-tag news endtag=required>
<div class="news">
  %body
</div>
</define-tag>

<define-tag news-title endtag=required>
<div class="news-title">
  <strong>%body</strong>
</div>
</define-tag>

<define-tag news-body endtag=required>
<div class="news-body">
  %body
</div>
</define-tag>

<define-tag news-entry endtag=required>
<div class="news-entry">
  %body
</div>
</define-tag>

<define-tag news-file>
<include command="wml/scripts/news2wml $(WML_SRC_BASENAME).news" />
</define-tag>

